{

   "name": "RestDoccer Examples",

   "endpoints": [

      {
         "endpoint":   "/user/:id",
         "method": "POST",
         "description": "Get some user object thingy by ID.",
         "urlParameters": {
            "id": {
               "description": "ID of the user.",
               "type":        "integer"
            }
         },
         "postParameters": {
            "getHidden": {
               "description": "Return hidden data.",
               "type":        "boolean",
               "required":    "false"
            }
         },
         "responses": {

            "success": {

               "username": "testuser",
               "email": "testemail@example.com"

            },

            "error": {

               "error": "Could not find user with that ID."

            }

         }
      },

      {
         "endpoint": "/deprecated",
         "method": "GET",
         "deprecated": true,
         "postParameters": {
            "SomeWeirdPOSTParam": {
               "description": "Weird POST param..",
               "type":        "boolean",
               "required":    "true"
            }
         }
      },

      {
         "endpoint":   "/someExperimentalEndpointThatIsGoingToChange",
         "method": "POST",
         "description": "You shouldn't expect this to be stable...",
         "experimental": true
      }

   ]


}