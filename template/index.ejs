<!doctype html>
<html>
<head>
  <title> <%= info.name %> </title>
</head>
<body>
<div class="navbar navbar-default">
  <div class="container">
    <div class="navbar-header">
      <a href="<%= info.apiUrl %>" class="navbar-brand"><%= info.name %></a>
    </div>
  </div>
</div>
<div class="container">
  <div class="accordion" id="apis">
    <% for(var i = 0; i < apis.length; i++) { %>
    <%- apis[i] %>
    <% } %>
  </div>
</div>

<footer>
  <div class="container">
    <% if(info.footer) { %>
    <div class="copyright"><%= info.footer %></div>
    <% } %>
  </div>
</footer>

</body>
</html>